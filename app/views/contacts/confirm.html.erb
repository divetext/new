<h3>以下の内容で、送信する。</h3>

<%= form_for @contact, :url => contacts_path do |f| %>
  <%= f.hidden_field :name %>
  <%= f.hidden_field :email %>
  <%= f.hidden_field :content %>
  <p>タイトル:<%= @contact.name %></p>
  <p>本文:<%= @contact.email %></p>
  <p>本文:<%= @contact.content %></p>
  <%= f.submit "登録する" %>
  <%= f.submit "戻る", name: "back" %>
<% end %>
